#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/Chenyx的移动硬盘/code-template/vue3-ts-template/node_modules/pidtree/bin/node_modules:/Volumes/Chenyx的移动硬盘/code-template/vue3-ts-template/node_modules/pidtree/node_modules:/Volumes/Chenyx的移动硬盘/code-template/vue3-ts-template/node_modules:/Volumes/Chenyx的移动硬盘/code-template/node_modules:/Volumes/Chenyx的移动硬盘/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Chenyx的移动硬盘/code-template/vue3-ts-template/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Volumes/Chenyx的移动硬盘/code-template/vue3-ts-template/node_modules/pidtree/bin/node_modules:/Volumes/Chenyx的移动硬盘/code-template/vue3-ts-template/node_modules/pidtree/node_modules:/Volumes/Chenyx的移动硬盘/code-template/vue3-ts-template/node_modules:/Volumes/Chenyx的移动硬盘/code-template/node_modules:/Volumes/Chenyx的移动硬盘/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Chenyx的移动硬盘/code-template/vue3-ts-template/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../pidtree/bin/pidtree.js" "$@"
else
  exec node  "$basedir/../pidtree/bin/pidtree.js" "$@"
fi
